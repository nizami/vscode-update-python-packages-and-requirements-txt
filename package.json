{
  "name": "update-python-packages-and-requirements-txt",
  "displayName": "Update python packages and requirements.txt",
  "description": "Update python packages and requirements.txt",
  "publisher": "nizami",
  "license": "MIT",
  "version": "1.1.4",
  "engines": {
    "vscode": "^1.57.0"
  },
  "bugs": {
    "url": "https://github.com/nizami/vscode-update-python-packages-and-requirements-txt/issues"
  },
  "homepage": "https://github.com/nizami/vscode-update-python-packages-and-requirements-txt#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/nizami/vscode-update-python-packages-and-requirements-txt.git"
  },
  "icon": "icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:update-python-packages-and-requirements-txt-with-an-existing-terminal",
    "onCommand:update-python-packages-and-requirements-txt-with-a-new-terminal"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "update-python-packages-and-requirements-txt-with-an-existing-terminal",
        "title": "Update Python packages and requirements.txt with an existing terminal"
      },
      {
        "command": "update-python-packages-and-requirements-txt-with-a-new-terminal",
        "title": "Update Python packages and requirements.txt with a new terminal"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "^16.11.11",
    "@types/vscode": "^1.57.0",
    "typescript": "^4.5.2"
  }
